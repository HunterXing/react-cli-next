    "lint:fix": "eslint \"./**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.+(js|jsx|ts|tsx)\"",
    "prepare": "husky install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "commitlint": "commitlint --edit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "husky": {
    "hooks": {
